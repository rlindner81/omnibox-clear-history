{
  "manifest_version": 3,
  "homepage_url": "https://github.com/rlindner81/omnibox-clear-history",
  "name": "Omnibox Clear History",
  "description": "Omnibox Clear History - Clear Omnibox Google Search History Suggestions",
  "version": "1.0",
  "permissions": ["history"],
  "action": {
    "default_icon": "assets/icon_24.png"
  },
  "background": {
    "service_worker": "src/clear-history.js"
  },
  "icons": {
    "16": "assets/icon_16.png",
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  }
}
